.content {
  background: #f9f9f9;
  height: 100%;
  padding: 0 20rpx;
}
.content .header {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
}
.content .header .title {
      font-size: 32rpx;
      color: #43454e;
}
.content .header .btntxt {
      font-size: 28rpx;
      color: #9d9ea4;
}
.content .header .column {
      width: 2rpx;
      height: 26rpx;
      background-color: #9c9da3;
      margin: 0 20rpx;
}
.content .header .icon {
      width: 39rpx;
      height: 39rpx;
      border-radius: 50px;
}
.content .item {
    background-color: #ffffff;
    -webkit-box-shadow: 0px 3px 16px 5px rgba(20, 0, 43, 0.08);
            box-shadow: 0px 3px 16px 5px rgba(20, 0, 43, 0.08);
    border-radius: 10rpx;
    margin-bottom: 30rpx;
}
.content .item .itemimg {
      width: 338rpx;
      height: 282rpx;
}
.content .item .itemmain {
      margin: 20rpx;
      width: 325rpx;
}
.content .item .itemmain .mt10 {
        margin-top: 10rpx;
}
.content .item .itemmain .recommend {
        width: 100rpx;
        height: 30rpx;
        background-color: #fd4a74;
        border-radius: 4rpx;
        font-size: 20rpx;
        color: #fff;
        text-align: center;
        line-height: 30rpx;
}
.content .item .itemmain .trybtn {
        width: 50rpx;
        height: 30rpx;
        border-radius: 4rpx;
        border: solid 1px #fd4a74;
        line-height: 30rpx;
        font-size: 20rpx;
        color: #fd4a74;
        text-align: center;
        margin-left: 10rpx;
}
.content .item .itemmain .title {
        font-size: 24rpx;
        color: #666879;
        width: 270rpx;
}
.content .item .itemmain .fit {
        width: 50rpx;
        height: 30rpx;
        border-radius: 4px;
        line-height: 30rpx;
        border: solid 1px #35dadc;
        font-size: 20rpx;
        color: #35dadc;
        text-align: center;
}
.content .item .itemmain .describe {
        font-size: 24rpx;
        color: #666879;
}
.content .item .itemmain .sku {
        margin-top: 20rpx;
}
.content .item .itemmain .sku .moneryicon {
          font-size: 20rpx;
          color: #fd4a74;
}
.content .item .itemmain .sku .monery {
          font-size: 36rpx;
          color: #fd4a74;
          line-height: 35rpx;
}
.content .item .itemmain .sku .skutxt {
          font-size: 22rpx;
          color: #c2c3ce;
}
.content .item .itemmain .addcartbtn {
        width: 194rpx;
        height: 46rpx;
        border-radius: 50px;
        border: solid 1px #35dadc;
        font-size: 24rpx;
        margin-top: 30rpx;
        color: #35dadc;
        padding: 0 10rpx;
}
.content .item .itemmain .addcartbtn .loveicon {
          width: 26rpx;
          height: 22rpx;
          margin-right: 10rpx;
}

